    <number>605-19</number>
    <name>EDGAR Filer Manual v68d 6.5.19</name>
    <description>
        <a href="https://www.sec.gov/files/edgar/filermanual/archive/edgarfm-vol2-v68-draft.pdf#6.5.19  Required contexts">Refer to EFM: 6.5.19  Required contexts</a>
    </description>
    <reference specification="10980"></reference>
    <variation id="_000gd">
        <name>6.5.19 Full period context in 10K, GOOD</name>
        <description>Full period context in 10K, GOOD</description>
        <data>
            <parameter name="ELOparams" value='{ "periodOfReport" : "12-31-2008", "submissionType" : "10-K" }'></parameter>
            <instance readMeFirst="true">e60519000gd-20081231.xml</instance>
            <linkbase>edgar-20081231_pre.xml</linkbase>
            <linkbase>edgar-20081231_def.xml</linkbase>
            <linkbase>edgar-20081231_lab.xml</linkbase>
            <schema>edgar-20081231.xsd</schema>
        </data>
        <result expected="valid"></result>
    </variation>
    <variation id="_001ng">
        <name>6.5.19 There are three contexts, but none are default contexts, NOGOOD.</name>
        <description>Each of the three contexts has a segment, so there are no default contexts, NOGOOD. Note that this will also cause 6.5.20 errors and 6.5.40 warnings because certain required facts must be found in the default context, which does not exist.</description>
        <reference specification="10980"></reference>
        <data>
            <parameter name="ELOparams" value='{ "periodOfReport" : "12-31-2008", "emergingGrowthCompanyFlag" : false, "submissionType" : "8-K" }'></parameter>
            <instance readMeFirst="true">e60519001ng-20081231.xml</instance>
            <linkbase>edgar-20081231_pre.xml</linkbase>
            <linkbase>edgar-20081231_def.xml</linkbase>
            <linkbase>edgar-20081231_lab.xml</linkbase>
            <schema>edgar-20081231.xsd</schema>
        </data>
        <result>
            <assert severity="err" num="60519" name="Required-Context-Missing" frd="cp"></assert>
        </result>
    </variation>
    <variation id="_002gd">
        <name>6.5.19 One day context in 485BPOS, which has no 'reporting period', GOOD</name>
        <description>One day context in 485BPOS, which has no 'reporting period', GOOD</description>
        <reference specification="10980"></reference>
        <data>
            <parameter name="ELOparams" value='{ "rptSeriesClassInfo.seriesIds" : ["S013456789"], "invCompanyType" : "N-1A", "submissionType" : "485BPOS" }'></parameter>
            <instance readMeFirst="true">e60519002gd-20081231.xml</instance>
            <linkbase>edgar-20081231_pre.xml</linkbase>
            <linkbase>edgar-20081231_def.xml</linkbase>
            <linkbase>edgar-20081231_lab.xml</linkbase>
            <schema>edgar-20081231.xsd</schema>
        </data>
        <result expected="valid"></result>
    </variation>
    <variation id="_004gd">
        <name>6.5.19 Full period context in 10K, GOOD</name>
        <description>Full period context in 10K, GOOD</description>
        <data>
            <parameter name="ELOparams" value='{ "periodOfReport" : "12-31-2008", "submissionType" : "10-K" }'></parameter>
            <instance readMeFirst="true">e60519004gd-20091231.xml</instance>
