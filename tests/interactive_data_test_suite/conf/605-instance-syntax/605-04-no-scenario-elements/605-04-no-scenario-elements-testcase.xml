    <number>605-04</number>
    <name>EDGAR Filer Manual v68d 6.5.4</name>
    <description>
        <a href="https://www.sec.gov/files/edgar/filermanual/archive/edgarfm-vol2-v68-draft.pdf#6.5.4  The xbrli:scenario element must not appear in any xbrli:context.">Refer to EFM: 6.5.4  The xbrli:scenario element must not appear in any xbrli:context.</a>
    </description>
    <variation id="_000gd">
        <name>6.5.4, Dimension is in segment, GOOD</name>
        <description>6.5.4 Tests that no error is reported with segment dimension, GOOD </description>
        <reference specification="10491.15.2"></reference>
        <data>
            <parameter name="ELOparams" value='{ "periodOfReport" : "06-30-2011", "emergingGrowthCompanyFlag" : false, "submissionType" : "8-K" }'></parameter>
            <instance readMeFirst="true">e60504000gd-20111231.xml</instance>
            <linkbase>e60504000gd-20111231_lab.xml</linkbase>
            <linkbase>e60504000gd-20111231_def.xml</linkbase>
            <linkbase>e60504000gd-20111231_pre.xml</linkbase>
            <schema>e60504000gd-20111231.xsd</schema>
        </data>
        <result expected="valid"></result>
    </variation>
    <variation id="_001ng">
        <name>6.5.4, Dimension is reported in scenario to raise error, NOGOOD</name>
        <description>6.5.4 Tests that scenario dimension reports error, NOGOOD</description>
        <reference specification="10491.15.2"></reference>
        <reference specification="11075"></reference>
        <data>
            <parameter name="ELOparams" value='{ "periodOfReport" : "06-30-2011", "emergingGrowthCompanyFlag" : false, "submissionType" : "8-K" }'></parameter>
