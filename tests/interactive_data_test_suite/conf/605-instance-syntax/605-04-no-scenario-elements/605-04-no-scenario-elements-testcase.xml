            <linkbase>e60504001ng-20111231_pre.xml</linkbase>
            <schema>e60504001ng-20111231.xsd</schema>
        </data>
        <result>
            <assert severity="err" num="60504" name="No-Scenario" frd="cp"></assert>
        </result>
    </variation>
    <variation id="_002ng">
        <name>6.5.4, Arbitrary user-defined dimension is reported in scenario to raise error, NOGOOD</name>
        <description>Arbitrary user-defined dimension is reported in scneario to raise error, NOGOOD</description>
        <reference specification="10491.15.2"></reference>
