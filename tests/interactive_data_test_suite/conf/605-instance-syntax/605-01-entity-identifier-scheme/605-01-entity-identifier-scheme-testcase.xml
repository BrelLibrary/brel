<?xml version='1.0' encoding='utf-8'?>
<?xml-stylesheet  type="text/xsl" href="../../../lib/test.xsl"?>
<testcase xmlns="http://edgar/2009/conformance" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://edgar/2009/conformance ../../../lib/test.xsd">
    <creator>
        <name>SEC Office of Structured Disclosure</name>
        <email>StructuredData@sec.gov</email>
    </creator>
    <number>605-01</number>
    <name>EDGAR Filer Manual v68d 6.5.1</name>
    <description>
        <a href="https://www.sec.gov/files/edgar/filermanual/archive/edgarfm-vol2-v68-draft.pdf#6.5.1  The scheme attribute of the xbrli:identifier element must be http://www.sec.gov/CIK.">Refer to EFM: 6.5.1  The scheme attribute of the xbrli:identifier element must be http://www.sec.gov/CIK.</a>
    </description>
    <reference specification="10491.2.2"></reference>
    <variation id="_000gd">
        <name>6.5.1, Scheme in instance is http://www.sec.gov/CIK, GOOD.</name>
        <description>6.5.1, Good scheme, GOOD.</description>
        <reference specification="10491.2.2"></reference>
        <data>
            <parameter name="ELOparams" value='{ "periodOfReport" : "06-30-2011", "emergingGrowthCompanyFlag" : false, "submissionType" : "8-K" }'></parameter>
            <instance readMeFirst="true">e60501000gd-20111231.xml</instance>
            <linkbase>e60501000gd-20111231_lab.xml</linkbase>
            <linkbase>e60501000gd-20111231_def.xml</linkbase>
            <linkbase>e60501000gd-20111231_pre.xml</linkbase>
            <schema>e60501000gd-20111231.xsd</schema>
        </data>
        <result expected="valid"></result>
    </variation>
    <variation id="_001ng">
        <name>6.5.1, Scheme in instance is http://www.sec.gov, NOGOOD.</name>
        <description>6.5.1, Bad Scheme, NOGOOD.</description>
        <reference specification="10491.2.2"></reference>
        <data>
            <parameter name="ELOparams" value='{ "periodOfReport" : "06-30-2011", "emergingGrowthCompanyFlag" : false, "submissionType" : "8-K" }'></parameter>
            <instance readMeFirst="true">e60501001ng-20111231.xml</instance>
            <linkbase>e60501001ng-20111231_lab.xml</linkbase>
            <linkbase>e60501001ng-20111231_def.xml</linkbase>
            <linkbase>e60501001ng-20111231_pre.xml</linkbase>
            <schema>e60501001ng-20111231.xsd</schema>
        </data>
        <result>
            <assert severity="err" num="60501" name="Entity-Identifier-Scheme" frd="cp"></assert>
        </result>
    </variation>
</testcase>
