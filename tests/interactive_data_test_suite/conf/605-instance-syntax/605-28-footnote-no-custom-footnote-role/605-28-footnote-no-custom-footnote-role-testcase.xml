    <number>605-28</number>
    <name>EDGAR Filer Manual v68d 6.5.28</name>
    <description>
        <a href="https://www.sec.gov/files/edgar/filermanual/archive/edgarfm-vol2-v68-draft.pdf#6.5.28 The role attribute of a footnote element must be defined in XBRL 2.1.">Refer to EFM: 6.5.28 The role attribute of a footnote element must be defined in XBRL 2.1.</a>
    </description>
    <reference specification="10994"></reference>
    <variation id="_000ng">
        <name>6.5.28 Instance footnote contains a custom footnote role, NOGOOD</name>
        <description>Instance footnote contains a custom footnote role, NOGOOD</description>
        <reference specification="10994"></reference>
        <data>
            <parameter name="ELOparams" value='{"periodOfReport": "12-31-2008", "submissionType": "8-K"}'></parameter>
            <instance readMeFirst="true">e60528000ng-20081231.xml</instance>
            <linkbase>edgar-20081231_pre.xml</linkbase>
            <linkbase>edgar-20081231_def.xml</linkbase>
            <linkbase>edgar-20081231_lab.xml</linkbase>
            <schema>edgar-20081231.xsd</schema>
            <schema>e60528000ng-20081231.xsd</schema>
        </data>
        <result>
            <assert severity="err" num="60528" name="Footnote-Custom-Footnote-Role" frd="du"></assert>
        </result>
    </variation>
    <variation id="_001ng">
        <name>6.5.28 Instance footnote contains footnote with no role, NOGOOD</name>
        <description>Instance footnote contains footnote with no role, NOGOOD.</description>
        <reference specification="10994"></reference>
        <reference specification="11152.11"></reference>
        <data>
            <parameter name="ELOparams" value='{"periodOfReport": "12-31-2008", "submissionType": "8-K"}'></parameter>
            <instance readMeFirst="true">e60528001ng-20081231.xml</instance>
            <linkbase>edgar-20081231_pre.xml</linkbase>
            <linkbase>edgar-20081231_def.xml</linkbase>
            <linkbase>edgar-20081231_lab.xml</linkbase>
            <schema>edgar-20081231.xsd</schema>
        </data>
        <result>
            <assert severity="err" num="60528" name="Footnote-Role-Missing" frd="fs"></assert>
        </result>
    </variation>
    <variation id="_002gd">
        <name>6.5.28 Instance footnoteLink contains a custom footnoteLink role, GOOD</name>
        <description>Instance footnote contains a custom footnoteLink role, GOOD</description>
        <reference specification="10994"></reference>
        <data>
            <parameter name="ELOparams" value='{"periodOfReport": "12-31-2008", "submissionType": "8-K"}'></parameter>
            <instance readMeFirst="true">e60528002gd-20081231.xml</instance>
            <linkbase>edgar-20081231_pre.xml</linkbase>
            <linkbase>edgar-20081231_def.xml</linkbase>
