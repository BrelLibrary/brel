    <reference specification="18153"></reference>
    <variation id="_000gd">
        <name>A simple table with two durations having a common end date.</name>
        <description>Same as the example in EFM 6.24.8</description>
        <data>
            <parameter name="ELOparams" value='{"periodOfReport": "12-31-2013", "emergingGrowthCompanyFlag" : false, "submissionType": "8-K"}'></parameter>
            <instance readMeFirst="true">000gd/r08000gd-20081231.xml</instance>
            <linkbase>000gd/r08000gd-20081231_cal.xml</linkbase>
            <linkbase>000gd/r08000gd-20081231_def.xml</linkbase>
            <linkbase>000gd/r08000gd-20081231_lab.xml</linkbase>
            <linkbase>000gd/r08000gd-20081231_pre.xml</linkbase>
            <schema>000gd/r08000gd-20081231.xsd</schema>
        </data>
        <result expected="valid"></result>
    </variation>
    <variation id="_001gd">
        <name>Three columns merging 1-year durations with instants at the end of each.</name>
        <description>This example uses the public float and number of employees figures.</description>
        <data>
            <parameter name="ELOparams" value='{"periodOfReport": "12-31-2015", "emergingGrowthCompanyFlag" : false, "submissionType": "10-K"}'></parameter>
