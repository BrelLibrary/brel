        <result>
            <assert severity="err" num="99999" name="xml.3.3.1:idMustBeUnique" frd="ix" fj="IXBRL_DUPLICATE_ELEMENT_ID_FOUND" nnum="70109" txt="Element $ id * is duplicated"></assert>
        </result>
    </variation>
    <variation id="_104ng">
        <name>, NOGOOD</name>
        <description>FAIL-footnote-dangling-continuation</description>
        <reference xiispec="6.1.2"></reference>
        <reference xiispec="4.1.2"></reference>
        <reference specification="If the continuedAt attribute is present, there MUST exist an ix:continuation element in the Inline XBRL Document Set with an {id} property which has a value matching the {continuation reference} property."></reference>
        <reference xiitest="FAIL-footnote-dangling-continuation"></reference>
        <data>
            <parameter name="ELOparams" value='{ "periodOfReport" : "12-31-2008", "emergingGrowthCompanyFlag" : false, "submissionType" : "8-K" }'></parameter>
            <instance readMeFirst="true">i09104ng/i09104ng-20081231.htm</instance>
            <linkbase>i09104ng/i09104ng-20081231_pre.xml</linkbase>
            <linkbase>i09104ng/i09104ng-20081231_lab.xml</linkbase>
            <schema>i09104ng/i09104ng-20081231.xsd</schema>
        </data>
        <result>
            <assert severity="err" num="99999" name="ix11.6.1.2:continuationMissing" frd="ix" fj="IXBRL_FOOTNOTE_CONTINUATION_MISSING" nnum="70109" txt="ix:footnote continuation link is missing."></assert>
        </result>
    </variation>
