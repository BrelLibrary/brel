    <variation id="_100ng">
        <name>Spurious attributes on ix:footnote, NOGOOD</name>
        <description>FAIL-footnote-any-attribute</description>
        <reference xiispec="6.1"></reference>
        <reference specification="ix:footnote can have any attribute with a namespace name which has the value http://www.w3.org/XML/1998/namespace"></reference>
        <reference xiitest="FAIL-footnote-any-attribute"></reference>
        <data>
            <parameter name="ELOparams" value='{ "periodOfReport" : "12-31-2008", "emergingGrowthCompanyFlag" : false, "submissionType" : "8-K" }'></parameter>
            <instance readMeFirst="true">i09100ng/i09100ng-20081231.htm</instance>
            <linkbase>i09100ng/i09100ng-20081231_pre.xml</linkbase>
            <linkbase>i09100ng/i09100ng-20081231_lab.xml</linkbase>
            <schema>i09100ng/i09100ng-20081231.xsd</schema>
        </data>
        <result>
            <assert severity="err" num="99999" name="html:syntaxError" frd="ix" fj="IXBRL_FOOTNOTE_IX_NS_ATTRIBUTE_NOT_ALLOWED" nnum="70109" txt="ix:footnote can not have attribute with ix namespace."></assert>
        </result>
    </variation>
    <variation id="_101ng">
        <name>Missing id attribute on ix:footnote, NOGOOD</name>
        <description>FAIL-footnote-missing-footnoteID</description>
        <reference xiispec="6.1.1"></reference>
        <reference specification="The ix:footnote element MUST have an id attribute."></reference>
        <reference xiitest="FAIL-footnote-missing-footnoteID"></reference>
        <data>
            <parameter name="ELOparams" value='{ "periodOfReport" : "12-31-2008", "emergingGrowthCompanyFlag" : false, "submissionType" : "8-K" }'></parameter>
            <instance readMeFirst="true">i09101ng/i09101ng-20081231.htm</instance>
            <linkbase>i09101ng/i09101ng-20081231_pre.xml</linkbase>
            <linkbase>i09101ng/i09101ng-20081231_lab.xml</linkbase>
            <schema>i09101ng/i09101ng-20081231.xsd</schema>
        </data>
        <result>
            <assert severity="err" num="99999" name="html:syntaxError" frd="ix" fj="IXBRL_FOOTNOTE_ID_REQUIRED" nnum="70109" txt="ix:footnote id attribute is required."></assert>
        </result>
    </variation>
    <variation id="_102ng">
        <name>duplicate footnote ids, NOGOOD</name>
        <description>FAIL-footnote-duplicate-footnoteIDs</description>
        <reference xiispec="6.1.1"></reference>
        <reference specification="ID values MUST uniquely identify the elements which bear them."></reference>
        <reference xiitest="FAIL-footnote-duplicate-footnoteIDs"></reference>
        <data>
            <parameter name="ELOparams" value='{ "periodOfReport" : "12-31-2008", "emergingGrowthCompanyFlag" : false, "submissionType" : "8-K" }'></parameter>
            <instance readMeFirst="true">i09102ng/i09102ng-20081231.htm</instance>
            <linkbase>i09102ng/i09102ng-20081231_pre.xml</linkbase>
            <linkbase>i09102ng/i09102ng-20081231_lab.xml</linkbase>
            <schema>i09102ng/i09102ng-20081231.xsd</schema>
        </data>
        <result>
            <assert severity="err" num="99999" name="xml.3.3.1:idMustBeUnique" frd="ix" fj="IXBRL_SCHEMA_VIOLATION" nnum="70109" txt="Element $ id * is duplicated"></assert>
        </result>
    </variation>
    <variation id="_103ng">
        <name>id of footnote is shared with a unit, NOGOOD</name>
        <description>FAIL-element-ix-footnote-04</description>
