    <!-- TODO Incorporate _109ng -->
    <variation id="_001gd">
        <name>GOOD</name>
        <description>Various footnote good tests, set 1 </description>
        <reference xiispec="6"></reference>
        <reference specification="6 The footnote element"></reference>
        <reference xiitest="PASS-element-ix-footnote-03"></reference>
        <reference xiitest="PASS-footnote-valid-element-content"></reference>
        <reference xiitest="PASS-footnote-xml-base-xhtml-base-no-interaction"></reference>
        <reference xiitest="PASS-element-link-footnote-02"></reference>
        <reference xiitest="PASS-element-link-footnote-complete"></reference>
        <reference xiitest="PASS-element-link-footnote-complete-role-defs"></reference>
        <reference xiitest="PASS-element-link-footnote-footnoteLinkRole"></reference>
        <reference xiitest="PASS-element-link-footnote-footnoteRole"></reference>
        <reference xiitest="PASS-element-link-footnote-nothidden"></reference>
        <reference xiitest="PASS-element-link-footnote-resolved-uris"></reference>
        <reference xiitest="PASS-element-link-footnote-xhtml-content"></reference>
        <reference xiitest="PASS-element-link-footnote-xhtml-content-exclude"></reference>
        <reference xiitest="PASS-elements-footnote-and-nonNumeric-unresolvable-uris-in-exclude"></reference>
        <reference xiitest="PASS-footnote-relative-uris-object-tag"></reference>
        <reference xiitest="PASS-footnote-any-attribute"></reference>
        <reference xiitest="PASS-footnote-continuation"></reference>
        <reference xiitest="PASS-footnote-ix-exclude-content"></reference>
        <reference xiitest="PASS-footnote-on-nonFraction"></reference>
        <reference xiitest="PASS-footnote-uris-with-spaces"></reference>
        <reference xiitest="PASS-footnote-within-footnote"></reference>
        <reference xiitest="PASS-unused-footnote"></reference>
        <reference xiitest="PASS-footnote-footnoteRole-multiple-output"></reference>
        <reference xiitest="PASS-many-to-one-footnote-complete"></reference>
        <reference xiitest="PASS-two-footnotes-multiple-output"></reference>
        <reference xiitest="PASS-many-to-one-footnotes-multiple-outputs"></reference>
        <reference xiitest="PASS-multiple-outputs-check-dont-have-empty-footnoteLinks"></reference>
        <data>
            <parameter name="ELOparams" value='{ "periodOfReport" : "12-31-2008", "emergingGrowthCompanyFlag" : false, "submissionType" : "8-K" }'></parameter>
            <instance readMeFirst="true">i09001gd/i09001gd-20081231.htm</instance>
