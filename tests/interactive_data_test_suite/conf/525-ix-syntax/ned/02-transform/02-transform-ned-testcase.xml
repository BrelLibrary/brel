        </result>
    </variation>
    <variation id="_103ng">
        <name>Format attribute on nonFraction is ixt:country, NOGOOD</name>
        <description>FAIL-unrecognised-schema-type</description>
        <reference xiitest="FAIL-unrecognised-schema-type"></reference>
        <reference specification="The {format} property MUST match the expanded name implied by the value of the name attribute of a signature element in the relevant Transformation Rules Registry."></reference>
        <reference xiispec="10.1.1"></reference>
        <data>
            <parameter name="ELOparams" value='{ "periodOfReport" : "12-31-2008", "emergingGrowthCompanyFlag" : false, "submissionType" : "8-K" }'></parameter>
            <instance readMeFirst="true">i02103ng/i02103ng-20081231.htm</instance>
            <linkbase>i02103ng/i02103ng-20081231_pre.xml</linkbase>
            <linkbase>i02103ng/i02103ng-20081231_lab.xml</linkbase>
            <schema>i02103ng/i02103ng-20081231.xsd</schema>
        </data>
        <result>
            <assert severity="err" num="99999" name="ix11.10.1.2:invalidTransformation" frd="ix" fj="IXBRL_FORMAT_UNDEFINED" nnum="70102" txt="Fact * has unrecognized transformation name #"></assert>
        </result>
    </variation>
