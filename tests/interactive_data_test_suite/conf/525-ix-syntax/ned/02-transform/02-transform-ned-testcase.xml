        </result>
    </variation>
    <variation id="_102ng">
        <name>Nonnumeric with format ixt:datemonthdayyearen and misspelled month, NOGOOD</name>
        <description>FAIL-invalid-long-month</description>
        <reference specification="The {value} property MUST be valid in accordance with the {format input type} property."></reference>
        <reference xiispec="10.1.1"></reference>
        <data>
