            <schema>i15104ng/i15104ng-20081231.xsd</schema>
        </data>
        <result>
            <assert severity="err" num="99999" name="html:syntaxError" frd="ix" fj="IXBRL_SCHEMA_VIOLATION" nnum="70115" txt="Attribute {attribute} is not allowed to appear in element {element}."></assert>
        </result>
    </variation>
</testcase>
