<?xml version='1.0' encoding='utf-8'?>
<?xml-stylesheet  type="text/xsl" href="../../../../lib/test.xsl"?>
<testcase xmlns="http://edgar/2009/conformance" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://edgar/2009/conformance ../../../../lib/test.xsd">
    <creator>
        <name>SEC Office of Structured Disclosure</name>
        <email>StructuredData@sec.gov</email>
    </creator>
    <number>525-10</number>
    <name>Inline XBRL Test Suite v68d : Header tests, Not EDGAR Dependent</name>
    <description>
        <h2>Various "good" (gd) and "not good" (ng) test files that

			demonstrate non-compliant

			iXBRL Header usage</h2>
    </description>
    <variation id="_001gd">
        <name>Valid header test case, GOOD</name>
        <description>PASS-single-ix-header-multi-input</description>
        <reference xiispec="8.1"></reference>
        <reference xiitest="PASS-single-ix-header-multi-input"></reference>
        <data>
            <parameter name="ELOparams" value='{ "periodOfReport" : "12-31-2008", "emergingGrowthCompanyFlag" : false, "submissionType" : "8-K" }'></parameter>
            <instance readMeFirst="true">i10001gd/i10001gd-20081231.htm</instance>
            <linkbase>i10001gd/i10001gd-20081231_pre.xml</linkbase>
            <linkbase>i10001gd/i10001gd-20081231_lab.xml</linkbase>
            <schema>i10001gd/i10001gd-20081231.xsd</schema>
        </data>
        <result expected="valid"></result>
    </variation>
    <variation id="_101ng">
        <name>Empty header element, NOGOOD</name>
        <description>FAIL-empty header</description>
        <reference xiispec="8.1.3"></reference>
        <reference xiitest="FAIL-empty-header"></reference>
        <reference specification="There MUST be at least one ix:header element in the Inline XBRL Document."></reference>
        <data>
            <parameter name="ELOparams" value='{ "periodOfReport" : "12-31-2008", "emergingGrowthCompanyFlag" : false, "submissionType" : "8-K" }'></parameter>
            <instance readMeFirst="true">i10101ng/i10101ng-20081231.htm</instance>
            <linkbase>i10101ng/i10101ng-20081231_pre.xml</linkbase>
            <linkbase>i10101ng/i10101ng-20081231_lab.xml</linkbase>
            <schema>i10101ng/i10101ng-20081231.xsd</schema>
        </data>
        <result>
            <assert severity="err" num="99999" name="ix11.8.1.3:headerMissing" frd="ix" fj="IXBRL_MUST_HAVE_AT_LEAST_ONE_HEADER_ELEMENT" nnum="70110" txt="Inline XBRL document set must have at least one ix:header element"></assert>
        </result>
    </variation>
    <variation id="_102ng">
        <name>ix:header element is child of html head, NOGOOD</name>
        <description>FAIL-ix-header-child-of-html-header</description>
        <reference xiispec="8.1.1"></reference>
        <reference specification="The ix:header element MUST NOT be a descendant of an HTML head element."></reference>
        <reference xiitest="FAIL-ix-header-child-of-html-header"></reference>
        <data>
            <parameter name="ELOparams" value='{ "periodOfReport" : "12-31-2008", "emergingGrowthCompanyFlag" : false, "submissionType" : "8-K" }'></parameter>
            <instance readMeFirst="true">i10102ng/i10102ng-20081231.htm</instance>
            <linkbase>i10102ng/i10102ng-20081231_pre.xml</linkbase>
            <linkbase>i10102ng/i10102ng-20081231_lab.xml</linkbase>
            <schema>i10102ng/i10102ng-20081231.xsd</schema>
        </data>
        <result>
            <assert severity="err" num="99999" name="html:syntaxError" frd="ix" fj="IXBRL_SCHEMA_VIOLATION" nnum="70110" txt="Invalid content was found starting with element : $"></assert>
        </result>
    </variation>
    <variation id="_103ng">
        <name>The header element contains continuation and nonNumeric elements, NOGOOD</name>
        <description>misplaced-ix-element-in-header</description>
        <reference xiispec="8.1"></reference>
        <reference specification="The ix:header element can contain ix:hidden, ix:references and ix:resources in the content"></reference>
        <reference xiitest="misplaced-ix-element-in-header"></reference>
        <data>
            <parameter name="ELOparams" value='{ "periodOfReport" : "12-31-2008", "emergingGrowthCompanyFlag" : false, "submissionType" : "8-K" }'></parameter>
            <instance readMeFirst="true">i10103ng/i10103ng-20081231.htm</instance>
            <linkbase>i10103ng/i10103ng-20081231_pre.xml</linkbase>
            <linkbase>i10103ng/i10103ng-20081231_lab.xml</linkbase>
            <schema>i10103ng/i10103ng-20081231.xsd</schema>
        </data>
        <result>
            <assert severity="err" num="99999" name="html:syntaxError" frd="ix" fj="IXBRL_SCHEMA_VIOLATION" nnum="70110" txt="Invalid content was found starting with element : $"></assert>
        </result>
    </variation>
</testcase>
